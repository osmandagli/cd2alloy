sig date {}
sig float {}
sig Order {
  orderId: one int
  userId: one int
  orderDate: one date
  status: one string
  totalAmount: one float
  refers: lower_upper_set Person
}
fact {
  all a: Order | #a.refers >= 2 and #a.refers =< 4
}
sig Person {
  userId: one int
  username: one string
  email: one string
  passwordHash: one string
  address: one string
}
